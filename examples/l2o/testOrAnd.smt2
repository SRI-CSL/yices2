(set-logic QF_NRA)
(declare-fun x () Real)
(declare-fun y () Real)
(declare-fun z () Real)
(declare-fun w () Real)
(assert (or (= (* x x) (- 1)) (= (* (+ 1 x) x) (* y y))))
;(assert (= (* x x) (- 1)))
(assert (or (and (= x y) (= y w)) (and (= x w) (= y z))))
;(assert (= x y))
(assert (= (+ (* z z) (* w w)) 1))
(assert (>= (+ (* (- x z) (- x z)) (* (- y w) (- y w))) 1))
(check-sat)